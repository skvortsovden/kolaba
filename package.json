{
	"name": "kolaba",
	"version": "1.0.0",
	"description": "Obsidian plugin to sync notes between devices using Github repository",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "npm run test:ts",
		"test:compile": "tsc -p tests/tsconfig.json",
		"test:ts": "npm run test:compile && cucumber-js tests/features/ --require tests/dist/step_definitions/simple_steps.js --name \"Pulling remote-only files\"",
		"test:all": "npm run test:compile && cucumber-js tests/features/ --require tests/dist/step_definitions/simple_steps.js",
		"lint": "eslint main.ts",
		"lint:fix": "eslint main.ts --fix"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@cucumber/cucumber": "^11.3.0",
		"@testing-library/jest-dom": "^6.7.0",
		"@types/jest": "^30.0.0",
		"@types/node": "^16.18.126",
		"@types/sinon": "^17.0.4",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"obsidian": "latest",
		"sinon": "^21.0.0",
		"ts-jest": "^29.4.1",
		"ts-node": "^10.9.2",
		"tslib": "2.4.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"isomorphic-git": "^1.24.5"
	}
}
